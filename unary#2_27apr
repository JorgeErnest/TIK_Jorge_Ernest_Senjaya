#include <iostream>
#include <math.h>
using namespace std;

int main(){
    float a = sqrt(9);//menghitung akar kuadrat
    float b = cbrt(27);//menghitung akar kubik
    float c = pow(4,2);//menghitung pangkat
    
    //tambahkan dan tampilkan d=sin e=cos f=tan
    float d = sin(1);
    float e = cos(1);
    float f = tan(1);
    
    float g = round(d);
    
    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    cout<<d<<endl;
    cout<<e<<endl;
    cout<<f<<endl;
    cout<<g<<endl;
    
    
    
    return 0;
}
